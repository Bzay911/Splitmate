{
  "expo": {
    "name": "Splitshare",
    "slug": "Splitmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splitshare_playstore.png",
    "scheme": "splitmate",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splitshare_playstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#0bb754"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8cc11141-6144-4370-933c-0af42aac097c"
      }
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/splitshare_playstore.png",
      "splash": {
        "image": "./assets/images/splitshare_playstore.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "tabletImage": "./assets/images/splitshare_playstore.png"
      },
      "bundleIdentifier": "com.beejaya.Splitmate",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splitshare_playstore.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "splash": {
        "image": "./assets/images/splitshare_playstore.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.beejaya.Splitmate"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin/app.plugin.js",
        {
          "iosUrlScheme": "com.googleusercontent.apps.908503370224-lpv2uf8iosv76gtd65jlsd9k3r4rsl98"
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splitshare_playstore.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Splitmate to access your camera",
          "recordAudioAndroid": true
        }
      ],
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
